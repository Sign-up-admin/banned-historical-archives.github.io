    "name":  "banned-historical-archives.github.io",
    "version":  "0.1.0",
    "description":  "Banned Historical Archives - 数字历史档案馆",
    "type":  "module",
    "main":  "pages/index.tsx",
    "homepage":  "https://banned-historical-archives.github.io",
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/banned-historical-archives/banned-historical-archives.github.io.git"
                   },
    "bugs":  {
                 "url":  "https://github.com/banned-historical-archives/banned-historical-archives.github.io/issues"
             },
    "license":  "MIT",
    "engines":  {
                    "node":  "\u003e=20.0.0",
                    "npm":  "\u003e=10.0.0"
                },
    "keywords":  [
                     "historical-archives",
                     "digital-archive",
                     "react",
                     "nextjs",
                     "typescript"
                 ],
    "scripts":  {
                    "build-article-json":  "ts-node --project ./tsconfig.build.json ./backend/build-article-json.ts ; Write-Output \"\"",
                    "build-indexes":  "node scripts/build-indexes-standalone.cjs ; Write-Output \"\"",
                    "dev":  "next dev ; Write-Output \"\"",
                    "init-config":  "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository.ts config ; Write-Output \"\"",
                    "init-parsed":  "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository.ts parsed ; Write-Output \"\"",
                    "init-raw":  "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository.ts raw ; Write-Output \"\"",
                    "init-ocr_cache":  "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository ocr_cache ; Write-Output \"\"",
                    "init-ocr_patch":  "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository ocr_patch ; Write-Output \"\"",
                    "init-es":  "node scripts/init-es-standalone.cjs ; Write-Output \"\"",
                    "reset-es":  "node scripts/init-es-standalone.cjs reset ; Write-Output \"\"",
                    "check-es":  "node scripts/check-es.cjs ; Write-Output \"\"",
                    "hack-mui":  "sed -i\u0027\u0027 -e \u0027s/switch/return;switch/g\u0027 node_modules/@mui/x-license/Watermark/Watermark.js ; Write-Output \"\"",
                    "build":  "npm run hack-mui \u0026\u0026 next build ; Write-Output \"\"",
                    "build-txt":  "ts-node --project ./tsconfig.build.json ./backend/build-txt.ts ; Write-Output \"\"",
                    "cp-pdfjs":  "cp -r ./node_modules/pdfjs-dist ./public/ ; Write-Output \"\"",
                    "postbuild":  "ts-node --project ./tsconfig.build.json ./backend/build-sitemap.ts ; Write-Output \"\"",
                    "start":  "npm run init-es \u0026\u0026 npx serve@latest out ; Write-Output \"\"",
                    "lint":  "eslint \"**/*.{ts,tsx,js,jsx}\" --max-warnings 100 ; Write-Output \"\"",
                    "lint:fix":  "eslint \"**/*.{ts,tsx,js,jsx}\" --fix --max-warnings 100 ; Write-Output \"\"",
                    "type-check":  "powershell -Command \"tsc --noEmit --project tsconfig.json --skipLibCheck; Write-Output ''\"",
                    "lint:docs":  "npm run lint:docs:format \u0026\u0026 npm run lint:docs:links \u0026\u0026 npm run lint:docs:text ; Write-Output \"\"",
                    "lint:docs:format":  "markdownlint \"**/*.md\" --ignore node_modules --ignore .git --ignore out --ignore parsed --ignore config --ignore raw --ignore ocr_cache --ignore ocr_patch ; Write-Output \"\"",
                    "lint:docs:links":  "node scripts/check-markdown-links.js ; Write-Output \"\"",
                    "lint:docs:text":  "textlint \"**/*.md\" --ignore-path .textlintignore ; Write-Output \"\"",
                    "lint:docs:fix":  "markdownlint --fix \"**/*.md\" --ignore node_modules --ignore .git --ignore out --ignore parsed --ignore config --ignore raw --ignore ocr_cache --ignore ocr_patch ; Write-Output \"\"",
                    "prettier":  "prettier -w . ; Write-Output \"\"",
                    "generate:docs":  "ts-node --project ./tsconfig.build.json ./scripts/generate-docs.ts ; Write-Output \"\"",
                    "docs":  "npm run generate:docs ; Write-Output \"\"",
                    "test":  "vitest run ; Write-Output \"\"",
                    "test:watch":  "vitest watch ; Write-Output \"\"",
                    "test:coverage":  "vitest run --coverage ; Write-Output \"\"",
                    "test:ui":  "vitest --ui ; Write-Output \"\"",
                    "build:analyze":  "cross-env ANALYZE=true next build ; Write-Output \"\"",
                    "build:profile":  "next build --profile ; Write-Output \"\"",
                    "validate-env":  "node scripts/validate-env.js ; Write-Output \"\"",
                    "cf:deploy":  "wrangler pages deploy out --project-name=banned-historical-archives ; Write-Output \"\"",
                    "cf:dev":  "wrangler pages dev out --project-name=banned-historical-archives ; Write-Output \"\"",
                    "build:preview":  "NODE_ENV=development npm run build ; Write-Output \"\""
                },
